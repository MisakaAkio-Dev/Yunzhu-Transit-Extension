{
  "schemaVersion": 1,
  "id": "yte",
  "version": "${version}",
  "name": "Yunzhu Transit Extension",
  "description": "云竹交通\n云筑工坊交通拓展",
  "authors": [
    "\nXFunny\nMrNTkey\nQBYQ"
  ],
  "contact": {
    "homepage": "https://space.bilibili.com/3546779284932989",
    "sources": "https://github.com/X-Funny/Yunzhu-Transit-Extension"
  },
  "license": "MIT License",
  "icon": "assets/yte/icon.png",
  "environment": "*",
